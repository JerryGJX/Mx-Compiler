int test_while(){
    int i = 1, sum = 0;
    while(i <= 100){
        for(int j = 1; j <= 100; ++j)
            sum = sum + j;
    }
    return sum;
}

int main(){
    int sum = 0;
    for (int i = 1; i <= 100; ++i) {
        if (i <= 50)
            continue;
        else 
            break;
        sum++;
    }
    return 0;
}